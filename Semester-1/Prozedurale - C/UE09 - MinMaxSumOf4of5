#include <stdio.h>

int main() {
    int zahl[5];

    for (int i = 0; i < 5; i++) {
        scanf("%d", &zahl[i]);
    }

    int sum = zahl[0] + zahl[1] + zahl[2] + zahl[3] + zahl[4];
    int minNum = zahl[0];
    int maxNum = zahl[0];

    for (int i = 1; i < 5; i++) {
        if (zahl[i] < minNum) {
            minNum = zahl[i];
        }
        if (zahl[i] > maxNum) {
            maxNum = zahl[i];
        }
    }

    int minSum = sum - maxNum;
    int maxSum = sum - minNum;

    printf(": minimum sum: %d\nmaximum sum: %d", minSum, maxSum);

    return 0;
}


