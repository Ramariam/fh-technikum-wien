#include <stdio.h>

int nextPermutation(int arr[], int n);
void reverseArray(int arr[], int n);
void swap(int arr[], int i, int j);
void printArray(int arr[], int n);

int main()
{
    int n;

    printf(": ");
    scanf("%d", &n);
    printf(":\n");

    int arr[n];

    for (int i = 0; i < n; ++i)
    {
        scanf("%d", &arr[i]);
    }

    if (nextPermutation(arr, n))
    {
        printArray(arr, n);
    }
    else
    {
        printf("invalid permutation");
    }

    return 0;
}

int nextPermutation (int arr[], int n)
{
    int i = n - 1;

    while (i > 0 && arr[i - 1] >= arr[i])
    {
        --i;
    }

    if (i == 0)
    {
        reverseArray(arr, n);
        return 1;
    }
    else if (i < 0)
    {
        return 0;
    }
    else
    {
        
        int j = n - 1;

        while (arr[j] <= arr[i - 1])
        {
            --j;
        }
        swap(arr, i - 1, j);

        j = n - 1;

        while (i < j)
        {
            swap(arr, i, j);
            ++i;
            --j;
        }
    }
    return 1;
}

void reverseArray(int arr[], int n)
{
    for (int i = 0; i < n - 1; ++i)
    {
        for (int j = 0; j < n - i - 1; ++j)
        {
            if (arr[j] > arr[j + 1])
            {
                swap(arr, j, j + 1);
            }
        }
    }
}

void swap(int arr[], int i, int j)
{
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
}

void printArray(int arr[], int n)
{
    for (int i = 0; i < n; ++i)
    {
        printf("%d ", arr[i]);
    }
    printf("\n");
}
