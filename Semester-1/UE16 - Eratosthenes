#include <stdio.h>
#include <math.h> 

int main()
{
    int n;

    printf(": ");
    scanf("%d", &n);


    int prime[n + 1];

    for (int i = 0; i <= n; ++i)
    {
        prime[i] = 1;
    }

    
    for (int i = 2; i <= sqrt(n); ++i) 
    {
        if (prime[i] == 1)
        {
            for (int j = i * i; j <= n; j += i)
            {
                prime[j] = 0;
            }
        }
    }

    int counter = 0;


    for (int i = 2; i <= n; ++i)
    {
        if (prime[i] == 1)
        {
            printf("%d ", i);
            ++counter;

            if (counter % 10 == 0)
            {
                printf("\n");
            }
        }
    }
    return 0;
}
